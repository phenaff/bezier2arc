#!/bin/bash

base="/home/phn/Documents/WoodWorking/Projects/Live-edge-table"

function convert_file() {

infile=$base/$1/$2
outfile=$base/$1/$3

python -m bezier2arc --list -i $infile

python -m bezier2arc --convert -i $infile -o $outfile

python -m bezier2arc --list -i $outfile

}

convert_file table-1 gabarit-table-1.svg gabarit-table-1-arc.svg

